[9:32 pm, 14/07/2021] class Solution {
    
    private int addChar(char[] chars, char cur, int count, int index){
        
        chars[index++] = cur;
        
        if(count <= 1){
            return index;
        }
        
        String countStr = String.valueOf(count);
        int len = countStr.length();
                    
        int j = 0;
        while(j < len){
            char val = countStr.charAt(j);
            chars[index++] = val;
            j++;
        }
        
        return index;
    }
    
